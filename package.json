  "bugs": {
    "url": "https://github.com/klebergermano/react_express_mysql/issues"
  },
  "homepage": "https://github.com/klebergermano/react_express_mysql#readme",
  "dependencies": {
    "concurrently": "^5.0.0",
    "express": "^4.17.1",
    "nodemailer": "^6.4.2",
    "react-router-dom": "^5.1.2",
    "react-scroll": "^1.7.15",
    "react-scrollable-anchor": "^0.6.1"
  },
  "devDependencies": {
    "nodemon": "^2.0.1"
  }
